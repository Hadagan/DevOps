pipeline {
    agent {
    label 'aws-jenkins-slave-1'
    }
    stages { 
        stage('Example') {
            steps {
              sh '''#!/bin/bash
              echo "ты выбрал $PARAM_NAME"
              case in {$PARAM_NAME} '--git':)
              pwd;;
              '--env':)
              ls -la;;
              esac
         '''
            }
        }
    }
}
